
// Example 1

function setup() {
  createCanvas(400, 400);
  colorMode(HSB,360,100,100);
}

function draw() {
  background(120,100,100);
  circle(50,50,80);
  square(110,10,80);
}


// Example 2

function setup() {
  createCanvas(400, 400);
  colorMode(HSB,360,100,100);
}

function draw() {
  background(360);
  noStroke();
  fill(0,100,100,0.5);
  circle(100,100,80);
  fill(120,100,100,0.5);
  circle(125,140,80);
  fill(240,100,100,0.5);
  circle(75,140,80);
}


// Example 3

function setup() {
  createCanvas(400, 400);
  colorMode(HSB,360,100,100); 
}

function draw() {
  background(360);
  noStroke();
  fill(0,100,100,0.5);
  circle(100,100,80);
  fill(120,100,100,0.5);
  circle(125,140,80);
  fill(240,100,100,0.5);
  circle(75,140,80);
}


//Example 4

function setup() {
  createCanvas(400, 400);
  colorMode(HSB,360,100,100);
  noStroke();
}

function draw() {
  background(240,75,45);
  fill(0,0,100);
  circle(200,200,150);
  fill(120,75,45);
  circle(200,200,140);
  //star troubles
  push();
  translate(width * 0.5, height * 0.5);
  star(200,200,100,5);
  pop();
}

